  "exports": {
    ".": "./obj-ml.js",
    "./obj-ml.js": "./obj-ml.js",
    "./o-m.js": "./o-m.js",
    "./li-ml.js": "./li-ml.js"
  },
  "files": [
    "*.*"
  ],
  "typings": "types.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/obj-ml.git"
  },
  "dependencies": {
